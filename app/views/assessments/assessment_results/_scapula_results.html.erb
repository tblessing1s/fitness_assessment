<div class="page-break">
	<h2><center>Scapula</center><hr><h5><i>Deviation of Antagonist Muscles</i></h5></hr></h2>
	<table class= "table table-bordered">
		<tr>
			<th scope="col"> </th>
			<th scope="col"> Left </th>
			<th scope="col"> Right </th>
		</tr>
		<tr>
			<th scope="col"> Scapula Elevation </th>
			<td><%= @assessment.scapula.scapula_elevation_left %></td>
			<td><%= @assessment.scapula.scapula_elevation_right %></td>
			<td><%= muscle_balance(@assessment.scapula.elevation_difference[:category], @assessment.scapula.elevation_difference[:percentage]) %></td>
		</tr>
		<tr>
			<th scope="col"> Scapula Depression </th>
			<td><%= @assessment.scapula.scapula_depression_left %></td>
