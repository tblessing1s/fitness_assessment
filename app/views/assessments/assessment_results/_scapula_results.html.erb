		</tr>
		<tr>
			<td></td>
			<td><%= muscle_balance(@assessment.scapula.elevation_depression_difference_left[:category], @assessment.scapula.elevation_depression_difference_left[:percentage]) %></td>
			<td><%= muscle_balance(@assessment.scapula.elevation_depression_difference_right[:category], @assessment.scapula.elevation_depression_difference_right[:percentage]) %></td>
